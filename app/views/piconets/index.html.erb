<p id="notice"><%= notice %></p>

<h1>Piconets</h1>

<table>
  <thead>
    <tr>
      <th>Channel</th>
      <th>Master</th>
      <th>Timer</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piconets.each do |piconet| %>
      <tr>
        <td><%= piconet.channel_id %></td>
        <td><%= piconet.master_id %></td>
        <td><%= piconet.timer %></td>
        <td><%= piconet.name %></td>
        <td><%= link_to 'Show', piconet %></td>
        <td><%= link_to 'Edit', edit_piconet_path(piconet) %></td>
        <td><%= link_to 'Destroy', piconet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Piconet', new_piconet_path %>
